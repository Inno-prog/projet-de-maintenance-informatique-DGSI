      },
      {
        "name": "PRESTATAIRE",
        "description": "Prestataire de services",
        "composite": false,
        "clientRole": false,
        "containerId": "Maintenance-DGSI"
      },
      {
        "name": "CORRESPONDANT_INFORMATIQUE",
        "description": "Correspondant informatique",
        "composite": false,
        "clientRole": false,
        "containerId": "Maintenance-DGSI"
      }
    ]
  },
